

n=int(input())

L=[ [0 for i  in range(n+1) ] for j in range(n+1)]



for i in range(1,n+1):
    x=str()
    for j in range(1,n+1):
        x+=str(L[i][j])
        x+=" "
    print(x)


#OR

n=int(input())

L=[ [0 for i  in range(n+1) ] for j in range(n+1)]



for i in range(1,n+1):

    for j in range(1,n+1):
         print(L[i][j],end=' ')

    print()

    
